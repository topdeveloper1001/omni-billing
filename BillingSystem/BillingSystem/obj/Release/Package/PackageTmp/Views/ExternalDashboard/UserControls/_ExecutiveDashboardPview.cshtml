@*------------------------------------------Volume Stats starts---------------*@
<div class="panel panel-default collapsecss ourcollapse bordertable" style="@style">
    <div class="panel-heading" data-toggle="collapse" data-parent="#accordion" href="#collapseSTATISTICS">
        <h4 class="panel-title collapseTitle">
            <span>
                <img src="@Url.Content("~/images/list.png")">
            </span>
            Volume Section 1
            <a class="btn btn-default btn-sm pull-right collapse-btn">
                <span class="glyphicon glyphicon-chevron-down"></span>
            </a>
        </h4>
    </div>
    <div id="collapseSTATISTICS" class="panel-collapse collapse in">
        <div class="panel-body">
            <div class="row" style="margin: 10px 0;">
                <div class="col-sm-6 text-center"><b>Current Month</b></div>
                <div class="col-sm-6 text-center"><b>Year to date</b></div>
            </div>
            <div class="base-color statsdiv" id="divManualDashboardStats">
                @{ Html.RenderPartial("~/Views/ExternalDashboard/UserControls/_VolumeExecutiveStats.cshtml", Model.Section1List); }
            </div>
            <span class="inactive"> 
              @Html.ActionLink(" ", "ExportToExcel",
                             new
                             {
                                 type = "1"
                             },
                             new
                             {
                                 @class = "excelexportIcon1",
                                 @title = "Export as Excel",
                                 @id = "btnExportVolumeData",
                                 @onclick="ExportToExcel(1);"
                             })
                </span>
        </div>
    </div>
</div>

@*------------------------------------------Section 1 Remarks starts------------------*@
<div id="divSection1Remarks">
    <div class="panel panel-default collapsecss ourcollapse bordertable" style="@style">
        <div class="panel-heading" data-toggle="collapse" data-parent="#accordion" href="#colSection1Remarks">
