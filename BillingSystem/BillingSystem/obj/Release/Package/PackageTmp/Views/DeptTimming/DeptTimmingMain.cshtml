@using BillingSystem.Common
@using BillingSystem.Models














@model DeptTimmingView

@{
    ViewBag.Title = "DeptTimming View";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style type="text/css">
    .home {
        -webkit-box-shadow: 10px 10px 1px 0px;
        margin: 4px 31px 62px 41px;
        border: 6px solid #E6E6FA;
        box-shadow: 10px 10px 1px 0px;
        width: 150px;
        height: 65px;
        float: left;
        text-align: center;
        background-color: #5c7499;
    }
</style>


<div class="white-bg">
    <div class="panel panel-default collapsecss ourcollapse">
        <div class="panel-heading" data-toggle="collapse" data-parent="#accordion" href="#collapseDeptTimmingAddEdit">
            <h4 class="panel-title collapseTitle">
                <span class="glyphicon glyphicon-plus-sign"></span>Add DeptTimming
                <a class="btn btn-default btn-sm pull-right collapse-btn">
                    <span class="glyphicon glyphicon-chevron-down"></span>
                </a>
            </h4>
        </div>
        <div id="collapscollapseDeptTimmingAddEditeOne" class="panel-collapse collapse">
            <div class="panel-body">
                <div class="base-color">
                    <div class="form">                        
                        @{Html.RenderPartial("~/Views/DeptTimming/UserControls/_DeptTimmingAddEdit.cshtml", Model.CurrentDeptTimming);}
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="panel panel-default collapsecss ourcollapse bordertable">
        <div class="panel-heading" data-toggle="collapse" data-parent="#accordion" href="#collapseDeptTimmingList">
            <h4 class="panel-title collapseTitle">
                <span><img src="../images/list.png" /></span>DeptTimming List
                <a class="btn btn-default btn-sm pull-right collapse-btn">
                    <span class="glyphicon glyphicon-chevron-down"></span>
                </a>
            </h4>
        </div>
        <div id="collapseDeptTimmingList" class="panel-collapse collapse in tableImg">
            <div class="panel-body">
                <div class="base-color" id="DeptTimmingListDiv">
                    @{Html.RenderPartial("~/Views/DeptTimming/UserControls/_DeptTimmingList.cshtml", Model.DeptTimmingList);}
                </div>
            </div>
        </div>
    </div>
    <div class="clear"></div>
</div>
            
<script src="~/Scripts/DeptTimming/DeptTimming.js?@Helpers.CurrentAssemblyTicks"></script>
<script src="~/Scripts/Common/confirmEvents.js?@Helpers.CurrentAssemblyTicks"></script>