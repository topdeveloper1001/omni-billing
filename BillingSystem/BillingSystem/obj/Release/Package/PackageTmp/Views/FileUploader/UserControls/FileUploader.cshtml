<div class="panel panel-default collapsecss ourcollapse">
    <div class="panel-heading" data-toggle="collapse" data-parent="#accordion" href="#collapseFileUploaderAddEdit">
        <h4 class="panel-title collapseTitle">
            <span class="glyphicon glyphicon-plus-sign"></span>&nbsp;Upload Documents<a class="btn btn-default btn-sm pull-right collapse-btn">
                                                                                         <span class="glyphicon glyphicon-chevron-down"></span>
                                                                                     </a>
        </h4>
    </div>
    <div id="collapseFileUploaderAddEdit" class="panel-collapse collapse in">
        <div class="panel-body">
            <div class="base-color">
                <div class="form" id="divDocumentUpload">
                    @{Html.RenderPartial("~/Views/FileUploader/UserControls/_FilesAddEdit.cshtml", Model.CurrentAttachment);}
                </div>
            </div>
        </div>
    </div>
</div>

<div class="panel panel-default collapsecss ourcollapse bordertable">
    <div class="panel-heading" data-parent="#accordion" data-toggle="collapse" href="#collapseFileUploaderList">
        <h4 class="panel-title collapseTitle">
            <span>
                <img src="~/images/list.png">
            </span>Document's List
            <a class="btn btn-default btn-sm pull-right collapse-btn">
                <span class="glyphicon glyphicon-chevron-down"></span>
            </a>
        </h4>
    </div>
    <div id="collapseFileUploaderList" class="panel-collapse tableImg collapse in">
        <div class="panel-body">
            <div class="base-color">
                <div class="table-responsive scrollable-panel" id="divDocumentsGrid">
                    @{Html.RenderPartial("~/Views/FileUploader/UserControls/_FilesListing.cshtml", Model.Attachments);}
                </div>
            </div>
        </div>
    </div>
</div>

<div id="divhidefile" style="display: none;">
    <div class="popup-coverbg"></div>
    <div class="popup-frame">
        <div class="popup-main1" style="margin: 50px auto; width:72%;">
            <div class="popup-content" style="min-height: 555px;">
                <div class="popup-close" onclick="$('#divhidefile').hide(); $.validationEngine.closePrompt('.formError', true);">X</div>
                <div class="minHeight" style="min-height: 535px">
                    <div id="Documentdiv">
                        
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@if (Model.ViewType != null && Model.ViewType == "2" )
