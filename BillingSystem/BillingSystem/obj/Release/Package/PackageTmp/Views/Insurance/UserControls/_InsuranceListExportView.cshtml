@model List<BillingSystem.Model.InsuranceCompany>

<div class="form">
    <div class="table-responsive">
        @if (Model != null && Model.Any())
        {
            var grid = new WebGrid(Model, ajaxUpdateContainerId: "gridContent", canPage: false, canSort: false);
            <div id="gridContent">
                @grid.GetHtml("table table-grid", "gridHead", "gridFooter", "gridRow", "gridAltRow", columns: grid.Columns(
                                grid.Column("InsuranceCompanyLicenseNumber", "Payor ID"),
                                grid.Column("InsuranceCompanyName", "Company Name"),
                                grid.Column("InsuranceCompanyStreetAddress", "Address"),
                                grid.Column("InsuranceCompanyCity", "City"),
                                grid.Column("InsuranceCompanyZipCode", "Company ZipCode"),
                                grid.Column("InsuranceCompanyMainPhone", "Company Main Phone"),
                                grid.Column("InsuranceCompanyEmailAddress", "Email Address")))
            </div>
        }
        else
        {

            <h2>No Record found</h2>
        }
    </div>
</div>
