@using BillingSystem.Models
@model XFileHeaderView

@{
    ViewBag.Title = "XFileHeader View";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style type="text/css">
    .home {
        -webkit-box-shadow: 10px 10px 1px 0px;
        margin: 4px 31px 62px 41px;
        border: 6px solid #E6E6FA;
        box-shadow: 10px 10px 1px 0px;
        width: 150px;
        height: 65px;
        float: left;
        text-align: center;
        background-color: #5c7499;
    }
</style>
<script src="~/Scripts/XFileHeader/XFileHeader.js"></script>

<div class="white-bg">
    <div class="panel panel-default collapsecss ourcollapse bordertable">
        <div class="panel-heading" data-toggle="collapse" data-parent="#accordion" href="#collapseXFileHeaderList">
            <h4 class="panel-title collapseTitle">
                <span><img src="~/images/list.png" /></span>XFileHeader List
                <a class="btn btn-default btn-sm pull-right collapse-btn">
                    <span class="glyphicon glyphicon-chevron-down"></span>
                </a>
            </h4>
        </div>
        <div id="collapseXFileHeaderList" class="panel-collapse collapse in tableImg">
            <div class="panel-body scroller">
                <div class="base-color" id="XFileHeaderListDiv">
                    @{Html.RenderPartial("~/Views/XFileHeader/UserControls/_XFileHeaderList.cshtml", Model.XFileHeaderList);}
                </div>
            </div>
        </div>
    </div>
    <div class="clear"></div>
</div>
            
