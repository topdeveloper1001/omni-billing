@using BillingSystem.Model
@model PatientPreScheduling

<style>
    .btn-group {
        margin-left: 10px;
        margin-bottom: 20px;
    }

    .clear {
        clear: both;
    }
    select {
    margin-bottom: 10px;}
</style>
<div class="form" id="PatientPreSchedulingFormDiv">
    @*// BBCOLUMNSHERE*@
   <div class="col-sm-3">
      <div class="rowTable">
        <label>PatientPreSchedulingId:</label>
        @Html.TextBoxFor(m => m.PatientPreSchedulingId, new { @maxlength = "10", @id = "txtPatientPreSchedulingId" })
      </div>
      <div class="rowTable">
        <label>PatientId:</label>
        @Html.TextBoxFor(m => m.PatientId, new { @maxlength = "10", @id = "txtPatientId" })
      </div>
      <div class="rowTable">
        <label>EncounterId:</label>
        @Html.TextBoxFor(m => m.EncounterId, new { @maxlength = "10", @id = "txtEncounterId" })
      </div>
      <div class="rowTable">
        <label>FacilityId:</label>
        @Html.TextBoxFor(m => m.FacilityId, new { @maxlength = "10", @id = "txtFacilityId" })
      </div>
      <div class="rowTable">
        <label>PhysicianId:</label>
        @Html.TextBoxFor(m => m.PhysicianId, new { @maxlength = "100", @id = "txtPhysicianId" })
      </div>
   </div>
   <div class="col-sm-3">
      <div class="rowTable">
        <label>PhysicianSpeciality:</label>
        @Html.TextBoxFor(m => m.PhysicianSpeciality, new { @maxlength = "100", @id = "txtPhysicianSpeciality" })
      </div>
      <div class="rowTable">
        <label>TypeOfProcedure:</label>
        @Html.TextBoxFor(m => m.TypeOfProcedure, new { @maxlength = "100", @id = "txtTypeOfProcedure" })
      </div>
      <div class="rowTable">
        <label>CorporateId:</label>
        @Html.TextBoxFor(m => m.CorporateId, new { @maxlength = "10", @id = "txtCorporateId" })
      </div>
      <div class="rowTable">
        <label>Description:</label>
        @Html.TextBoxFor(m => m.Description, new { @maxlength = "500", @id = "txtDescription" })
      </div>
      <div class="rowTable">
        <label>CreatedBy:</label>
        @Html.TextBoxFor(m => m.CreatedBy, new { @maxlength = "10", @id = "txtCreatedBy" })
      </div>
   </div>
   <div class="col-sm-3">
      <div class="rowTable">
        <label>CreatedDate:</label>
        @Html.TextBoxFor(m => m.CreatedDate,"{0:MM/dd/yyyy}", new { @maxlength = "10", @id = "dtCreatedDate" })
      </div>
      <div class="rowTable">
        <label>ModifiedBy:</label>
        @Html.TextBoxFor(m => m.ModifiedBy, new { @maxlength = "10", @id = "txtModifiedBy" })
      </div>
      <div class="rowTable">
        <label>ModifiedDate:</label>
        @Html.TextBoxFor(m => m.ModifiedDate,"{0:MM/dd/yyyy}", new { @maxlength = "10", @id = "dtModifiedDate" })
      </div>
      <div class="rowTable">
        <label>IsActive:</label>
        @Html.TextBoxFor(m => m.IsActive, new { @maxlength = "10", @id = "txtIsActive" })
      </div>
      <div class="rowTable">
        <label>DeletedBy:</label>
        @Html.TextBoxFor(m => m.DeletedBy, new { @maxlength = "10", @id = "txtDeletedBy" })
      </div>
   </div>
   <div class="col-sm-3">
      <div class="rowTable">
        <label>DeletedDate:</label>
        @Html.TextBoxFor(m => m.DeletedDate,"{0:MM/dd/yyyy}", new { @maxlength = "10", @id = "dtDeletedDate" })
      </div>
      <div class="rowTable">
        <label>ExtValue1:</label>
        @Html.TextBoxFor(m => m.ExtValue1, new { @maxlength = "200", @id = "txtExtValue1" })
      </div>
      <div class="rowTable">
        <label>ExtValue2:</label>
        @Html.TextBoxFor(m => m.ExtValue2, new { @maxlength = "200", @id = "txtExtValue2" })
      </div>
      <div class="rowTable">
        <label>ExtValue3:</label>
        @Html.TextBoxFor(m => m.ExtValue3, new { @maxlength = "200", @id = "txtExtValue3" })
      </div>
      <div class="rowTable">
        <label>ExtValue4:</label>
        @Html.TextBoxFor(m => m.ExtValue4, new { @maxlength = "200", @id = "txtExtValue4" })
      </div>
   </div>
   <div class="col-sm-3">
      <div class="rowTable">
        <label>ExtValue5:</label>
        @Html.TextBoxFor(m => m.ExtValue5, new { @maxlength = "200", @id = "txtExtValue5" })
      </div>
      <div class="rowTable">
        <label>PreferredDate:</label>
        @Html.TextBoxFor(m => m.PreferredDate,"{0:MM/dd/yyyy}", new { @maxlength = "10", @id = "dtPreferredDate" })
      </div>
      <div class="rowTable">
        <label>PreferredTimeSlots:</label>
        @Html.TextBoxFor(m => m.PreferredTimeSlots, new { @maxlength = "50", @id = "txtPreferredTimeSlots" })
      </div>
    <div class="clear"></div>

    <div class="btn-group">
        <div style="float: left;">
            @if (Model.PatientPreSchedulingId != null && Model.PatientPreSchedulingId != 0)
            {  
                <input type="submit" class="btn btn-primary edit-btn marginBtn" value="Update" onclick="return SavePatientPreScheduling();" />                
            }
            else
            {
                <input type="submit" onclick="return SavePatientPreScheduling();" class="btn btn-primary edit-btn marginBtn" value="Save" />                
            }
            <button name="ClientCancel" class="btn btn-primary edit-btn marginBtn" type="button" onclick="return ClearAll();">Cancel</button>            
        </div>
    </div>



</div>
