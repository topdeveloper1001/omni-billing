@model BillingSystem.Models.RegisterPatientView

@{
    ViewBag.Title = "Register New Patient";
    Layout = "~/Views/Shared/_Layout.cshtml";

    var ticks = DateTime.Now.Ticks;
}

<div class="form">
    <div class="white-bg">
        <div class="panel panel-default collapsecss ourcollapse" id="divPatientInfo">
            <div class="panel-heading" data-toggle="collapse" data-parent="#accordion" href="#collapseOne" onclick="Div1OpenClose();">
                <h4 class="panel-title collapseTitle">
                    <span class="glyphicon glyphicon-info-sign"></span>&nbsp;Patient Information
                    <a class="btn btn-default btn-sm pull-right collapse-btn">
                        <span class="glyphicon glyphicon-chevron-down"></span>
                    </a>
                </h4>
            </div>
            <div id="collapseOne" class="panel-collapse collapse in">
                <form id="myForm" action='@Url.Action("AjaxSubmit", "ImagePreview")' method="post" enctype="multipart/form-data">
                    <div class="panel-body">
                        <div class="base-color patientInfoBg">
                            <div class="form patientTable">
                                <div class="clear"></div>
                                <div class="col-sm-6 col-md-3 col-lg-3">
                                    <div>
                                        <div class="rowTable">
                                            <label><span class="mandatoryStar"></span>First Name:</label>
                                            @Html.TextBoxFor(m => m.CurrentPatient.PatientInfo.PersonFirstName,
                                                     new { @maxlength = "25", @class = "validate[required]" })
                                        </div>
                                        <div class="rowTable">
                                            <label><span class="mandatoryStar"></span>Last Name:</label>
                                            @Html.TextBoxFor(m => m.CurrentPatient.PatientInfo.PersonLastName,
                                                     new { @maxlength = "25", @class = "validate[required]" })
                                        </div>
                                        <div class="rowTable">
                                            <div class="telephoneCol4">
                                                <label><span class="mandatoryStar"></span>Birth Date:</label>
                                                @Html.TextBoxFor(m => m.CurrentPatient.PatientInfo.PersonBirthDate, "{0:MM/dd/yyyy}",
                                                         new { @onchange = "CalculateAge(this),CheckValidDateOfBirth(this,'CurrentPatient_PatientInfo_PersonBirthDate');", @maxlength = "10", @class = "dtLessThanCurrent validate[required]" })
                                            </div>
                                            <div class="telephoneCol2">
                                                <label>Age:</label>
                                                @Html.TextBoxFor(m => m.CurrentPatient.PersonAge, new { @readonly = "true", @maxlength = "2" })
                                            </div>
                                        </div>
                                        <div class="rowTable">
                                            <label>Passport Number:</label>
                                            @Html.TextBoxFor(m => m.CurrentPatient.PatientInfo.PersonPassportNumber,
                                                     new { @maxlength = "15", @onchange = "CheckIfPassportAlreadyExists();" })
                                        </div>
                                        <div class="rowTable">
                                            <label>Passport Expiry Date:</label>
                                            @Html.TextBoxFor(m => m.CurrentPatient.PatientInfo.PersonPassportExpirtyDate, "{0:MM/dd/yyyy}",
                                                     new
                                                     {
                                                         @maxlength = "10",
                                                         @class = "dtGeneral",
                                                         @onchange = "CheckValidDateNotLessThanTodayDate(this,'CurrentPatient_PatientInfo_PersonPassportExpirtyDate')"
                                                     })
                                        </div>
                                        <div class="rowTable">
                                            <label>Social Security Number :</label>
                                            @Html.TextBoxFor(m => m.CurrentPatient.PatientInfo.PersonEmiratesIDNumber,
                                                     new
                                                     {
                                                         @maxlength = "11",
                                                         @class = "EmiratesMask validate[optional[validEmirates]]",
                                                         @placeholder = "111-11-1111",
                                                     })
                                        </div>
                                        <div class="rowTable">
                                            <label>Email:</label>
                                            @Html.TextBoxFor(m => m.PatientLoginDetail.Email, new { maxlength = "50", @class = "validate[optional[email]]" })
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-6 col-md-3 col-lg-3">
                                    <div class="rowTable">
                                        <label>Member ID:</label>
                                        @Html.TextBoxFor(m => m.Insurance.PersonHealthCareNumber, new { @maxlength = "10", @class = "validate[optional[nameFormat]]" })
                                    </div>
                                    <div class="rowTable">
                                        <label><span class="mandatoryStar"></span>Insurance Company:</label>
                                        <select id="ddlPersonInsuranceCompany" name="ddlPersonInsuranceCompany" class="validate[required]" onchange="BindInsuranceCompanyName();"></select>
                                        @Html.HiddenFor(m => m.Insurance.InsuranceCompanyId)
                                    </div>
                                    <div class="rowTable">
                                        <label><span class="mandatoryStar"></span>Insurance Plan:</label>
                                        <select id="ddlInsurancePlan" class="validate[required]" onchange="BindInsurancePolicies('');"></select>
                                        @Html.HiddenFor(m => m.Insurance.InsurancePlanId)
                                    </div>
                                    <div class="rowTable">
                                        <label><span class="mandatoryStar"></span>Insurance Policy :</label>
                                        <select id="ddlInsurancePolicy" class="validate[required]"></select>
                                        @Html.HiddenFor(m => m.Insurance.InsurancePolicyId)
                                    </div>
                                    <div class="rowTable">
                                        <div class="telephoneCol4">
                                            <label><span class="mandatoryStar"></span>Ins Start Date :</label>
                                            @Html.TextBoxFor(m => m.Insurance.Startdate, "{0:MM/dd/yyyy}", new { @maxlength = "10", @id = "InsStartDate", @class = "dtGeneral", @ClearTextBox = "True", @onchange = "CheckTwoDates($('#Insurance_Startdate'),$('#Insurance_Expirydate'),'Insurance_Startdate')" })
                                        </div>
                                        <div class="telephoneCol2">
                                            <label><span class="mandatoryStar"></span>Ins End Date :</label>
                                            @Html.TextBoxFor(m => m.Insurance.Expirydate, "{0:MM/dd/yyyy}", new { @maxlength = "10", @id = "InsEndtDate", @class = "dtGeneral", @ClearTextBox = "True", @onchange = "CheckTwoDates($('#Insurance_Startdate'),$('#Insurance_Expirydate'),'Insurance_Expirydate')" })
                                        </div>
                                    </div>
                                    <div class="rowTable">
                                        <label><span class="mandatoryStar"></span>Mobile Number:</label>
                                        <select id="ddlPersonContactNumber" class="marginBottomNone telephoneCol1 phoneddl" onchange="return OnCountryDropdownChange('#lblPersonContactNumber', '#ddlPersonContactNumber');"></select>
                                        <span id="lblPersonContactNumber" class="marginTopPhoneNo phnspan"></span>
                                        @Html.HiddenFor(m => m.CurrentPatient.PatientInfo.PersonContactNumber)
                                        <input type="text" class="PhoneMask telephoneCol2 validate[required,custom[phone]]" id="txtContactNumber" />
                                    </div>
                                    <div class="rowTable smallLabel">
                                        <label style="float: left;"><span></span>Patient Portal Access: </label>@Html.CheckBoxFor(m => m.PatientLoginDetail.PatientPortalAccess)
                                    </div>
                                </div>
                                <div class="col-sm-6 col-md-3 col-lg-3">
                                    <div class="rowTable smallLabel" id="divIsVIP">
                                        <label>Is VIP:</label>
                                        <div style="cursor: pointer; float: left; width: 80%;" title="For VIP Patient, please check the checkbox.">
                                            <div style="width: 10%; float: left;">
                                                @Html.CheckBox("PatientIsVIP", Model.CurrentPatient.PatientIsVIP ?? false, new { @id = "PatientIsVIP" })
                                            </div>
                                            <div style="width: 80%; float: left;" id="divCheckIsVip">
                                                @Html.TextAreaFor(m => m.CurrentPatient.PatientInfo.PersonVIP, new { @maxlength = "40", @style = "height: 44px;" })
                                                <img src="@Url.Content("~/images/vip2.png")" width="25">
                                                <div><span style="color: red;" id="spVIP"></span></div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="rowTable">
                                        <label><span class="mandatoryStar"></span>Nationality:</label>
                                        <select id="ddlPersonNationality" class="validate[required]"></select>
                                        @Html.HiddenFor(m => m.CurrentPatient.PatientInfo.PersonNationality)
                                    </div>
                                    <div class="rowTable">
                                        <label>National ID Expiration:</label>
                                        @Html.TextBoxFor(m => m.CurrentPatient.PatientInfo.PersonEmiratesIDExpiration, "{0:MM/dd/yyyy}"
                                       , new { @maxlength = "10", @class = "dtGeneral", @onchange = "CheckValidDateNotLessThanTodayDate(this,'txtPersonEmiratesIDExpiration')" })
                                    </div>

                                    <div class="rowTable">
                                        <label>Patient Financial Number:</label>
                                        @Html.TextBoxFor(m => m.CurrentPatient.PatientInfo.PersonFinancialNumber, new { @maxlength = "20" })
                                    </div>
                                    <div class="rowTable">
                                        <label>Master Patient Index Number:</label>
                                        @Html.TextBoxFor(m => m.CurrentPatient.PatientInfo.PersonMasterPatientNumber, new { @maxlength = "20" })
                                    </div>

                                    <div class="rowTable">
                                        <label>Medical Record No:</label>
                                        <div class="generateMedical">
                                            <a href="javascript:void(0);" title="Generate Medical Number" class="pull-right" id="labelAutoGenereteMedicalNumber" onclick="GetAutoGeneratedMedicalNumber();">
                                                <img src="@Url.Content("~/images/generateNo.png")" title="User can add a Medical Record Number manually or can click here to get the system-generated Medical Record Number." />
                                            </a>
                                            @Html.TextBoxFor(m => m.CurrentPatient.PatientInfo.PersonMedicalRecordNumber, new { @maxlength = "10", @onblur = "CheckMedicalNumberExist(this);", @class = "validate[optional[integer]]" })
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-6 col-md-3 col-lg-3">
                                    <div style="width: 100%; margin: 0 auto;">
                                        <div class="rowTable">
                                            <h1 class="profilePic">Profile Photo:</h1>
                                            <div class="userPicDiv">
                                                <div class="processingbar" id="imageProcessBar" style="display: none">
                                                    <img src="@Url.Content("~/images/loading.gif")" />
                                                </div>
                                                @if (@Model.CurrentPatient != null)
                                                {
                                                    if (!string.IsNullOrEmpty(@Model.CurrentPatient.ProfilePicImagePath) && @Model.CurrentPatient.DocumentTemplateId != null && @Model.CurrentPatient.DocumentTemplateId > 0)
                                                    {
                                                        <img id="pfimg" src="@Model.CurrentPatient.ProfilePicImagePath" class="user-img" style="width:185px;height:200px;" />
                                                        @Html.HiddenFor(m => m.CurrentPatient.DocumentTemplateId, new { @id = "hidDocumentTemplateId" })
                                                    }
                                                    else
                                                    {
                                                        <img id="pfimg" src="@Url.Content("~/images/Noimage.jpg")" class="user-img" style="width: 185px; height: 200px;" />
                                                    }
                                                }

                                                <div id="fupload">
                                                    <input type="file" onchange="ChangeImage('imageLoadPhoto', '#imgAddSettingLogo', '#div_ImageError', '#imageLoadPhoto', '#imgsub')" name="file" id="imageLoadPhoto" />
                                                    <input type="submit" value="submit" style="display: none;" />
                                                    <a onclick="SetSubmit()" id="imgsub" style="display: none;">ForceSubmit</a>
                                                </div>
                                            </div>
                                            <!-- start Cropping section -->
                                            <div id="divhidepopup1" style="display: none;">
                                                <div class="popup-coverbg"></div>
                                                <div class="popup-frame">
                                                    <div class="popup-main1" style="margin: 50px auto">
                                                        <div class="popup-content" style="min-height: 500px;">
                                                            <div class="popup-close" onclick="$('#divhidepopup1').hide(); $.validationEngine.closePrompt('.formError', true); CloseCropDiv();">X</div>
                                                            <div class="minHeight" style="min-height: 530px">
                                                                <div id="CropImage" class="croppopup">
                                                                    @*<span class="close" onmousedown="return CloseCropDiv();">X</span>*@
                                                                    <h3>crop</h3>
                                                                    <table border="0" cellpadding="0" cellspacing="0" height="400">
                                                                        <tr>
                                                                            <td valign="middle" align="center">

                                                                                <img id="ImgCropImage" style="cursor: pointer; vertical-align: middle; text-align: center; flex-align: center;" />
                                                                                <input type="hidden" id="X" />
                                                                                <input type="hidden" id="Y" />
                                                                                <input type="hidden" id="W" />
                                                                                <input type="hidden" id="H" />
                                                                            </td>
                                                                        </tr>
                                                                    </table>

                                                                    <div class="cropbtn-bar" id="div3">
                                                                        <input type="button" class="cropbtn blue-btn btn btn-primary edit-btn marginBtn" value="crop" onclick="cropImage(); $('#divhidepopup1').hide();" />
                                                                    </div>
                                                                </div>

                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                            <!-- end Cropping section -->
                                            <span id="spPersonPhoto" style="color: red;"></span>
                                            <div id="div_ImageError" style="color: red"></div>
                                        </div>
                                        <div class="rowTable smallLabel">
                                            <label>Sex:</label>
                                            <div class="">
                                                @Html.RadioButtonFor(model => model.CurrentPatient.PatientInfo.PersonGender, "Male", new { id = "male" })
                                                @Html.Label("male", "Male")
                                                @Html.RadioButtonFor(model => model.CurrentPatient.PatientInfo.PersonGender, "female", new { id = "female" })
                                                @Html.Label("female", "Female")
                                            </div>
                                        </div>
                                        <div class="rowTable smallLabel">
                                            <label>Marital Status:</label>
                                            <div>
                                                @Html.RadioButtonFor(model => model.CurrentPatient.PatientInfo.PersonMaritalStatus, "Married", new { id = "Married" })
                                                @Html.Label("Married", "Married")
                                                @Html.RadioButtonFor(model => model.CurrentPatient.PatientInfo.PersonMaritalStatus, "Single", new { id = "Single" })
                                                @Html.Label("Single", "Single")
                                            </div>
                                        </div>
                                        <div class="rowTable smallLabel">
                                            <div>
                                                <label>New Born:</label>
                                                <input type="checkbox" id="chkNewBaby" onclick="ShowActiveMotherDropdown();" />
                                            </div>
                                            <div id="divActiveMother">
                                                <label>Mother Name</label>
                                                @Html.HiddenFor(m => m.CurrentPatient.PatientInfo.PersonMotherName)
                                                <select id="ddlActiveMother"></select>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!--form end-->
                        </div>
                        <div class="btn-group">
                            <div id="overlay" class="web_dialog_overlay">
                            </div>
                            <img id="loading" src="~/images/gif-load (2).GIF" alt="Updating ..." style="display: none; position: fixed; left: 48%; top: 20%;" />
                            <button id="btnRegisterNewPatient" class="blue-btn btn btn-primary edit-btn marginBtn">Save</button>
                            <button id="btnCancelRegisterNewPatient" class="blue-btn btn btn-primary edit-btn marginBtn" onclick="ClearInfoForm();">Cancel</button>
                            <br />
                        </div>
                    </div>

                    @*For Address & Contacts Section *@
                    @Html.HiddenFor(m => m.CurrentPatientAddressRelation.PatientAddressRelationID)
                    @Html.HiddenFor(m => m.CurrentPatientAddressRelation.PatientAddressRelationType)
                    @Html.HiddenFor(m => m.CurrentPatientAddressRelation.FirstName)
                    @Html.HiddenFor(m => m.CurrentPatientAddressRelation.LastName)
                    @Html.HiddenFor(m => m.CurrentPatientAddressRelation.IsPrimary)
                    @Html.HiddenFor(m => m.CurrentPatientAddressRelation.StreetAddress1)
                    @Html.HiddenFor(m => m.CurrentPatientAddressRelation.StreetAddress2)
                    @Html.HiddenFor(m => m.CurrentPatientAddressRelation.POBox)
                    @Html.HiddenFor(m => m.CurrentPatientAddressRelation.CountryID, new { @id = "hdCountry", @class = "emptyddl" })
                    @Html.HiddenFor(m => m.CurrentPatientAddressRelation.StateID, new { @id = "hdState", @class = "emptyddl" })
                    @Html.HiddenFor(m => m.CurrentPatientAddressRelation.CityID, new { @id = "hdCity", @class = "emptyddl" })
                    @Html.HiddenFor(m => m.CurrentPatientAddressRelation.ZipCode)
                    @*For Address & Contacts Section *@


                    @*For Insurance Tab Start*@
                    @Html.HiddenFor(m => m.Insurance.PatientInsuraceID)
                    @Html.HiddenFor(m => m.Insurance.InsuranceCompanyId)
                    @Html.HiddenFor(m => m.Insurance.InsurancePlanId)
                    @Html.HiddenFor(m => m.Insurance.InsurancePolicyId)
                    @Html.HiddenFor(m => m.Insurance.CompanyName)
                    @Html.HiddenFor(m => m.Insurance.Startdate)
                    @Html.HiddenFor(m => m.Insurance.Expirydate)
                    @Html.HiddenFor(m => m.Insurance.PatientInsuranceId2)
                    @Html.HiddenFor(m => m.Insurance.CompanyId2)
                    @Html.HiddenFor(m => m.Insurance.PersonHealthCareNumber2)
                    @Html.HiddenFor(m => m.Insurance.Plan2)
                    @Html.HiddenFor(m => m.Insurance.Policy2)

                    @Html.HiddenFor(m => m.Insurance.StartDate2)
                    @Html.HiddenFor(m => m.Insurance.EndDate2)
                    @*For Insurance Tab End*@


                    @*For Documnet Tab *@
                    @Html.HiddenFor(m => m.DocumentsAttachment.DocumentTypeID)
                    @Html.HiddenFor(m => m.DocumentsAttachment.DocumentName)
                    @Html.HiddenFor(m => m.DocumentsAttachment.DocumentNotes)
                    @*END*@

                </form>
            </div>
        </div>
        <!--Additional Info Tab Start-->
        <div class="panel panel-default collapsecss ourcollapse moreinfo" id="divMoreInfo">
            <div class="panel-heading" data-toggle="collapse" data-parent="#accordion" href="#collapseTwo" onclick="DivClose();">
                <h4 class="panel-title collapseTitle">
                    <span class="glyphicon glyphicon-plus-sign"></span>&nbsp;Additional Info
                    <a class="btn btn-default btn-sm pull-right collapse-btn">
                        <span class="glyphicon glyphicon-chevron-down"></span>
                    </a>
                </h4>
            </div>
            <div id="collapseTwo" class="panel-collapse collapse in">
                <div class="panel-body">
                    <div class="base-color">
                        <div id="tabs">
                            <ul>
                                <li><a href="#AddressesTab" class="PatientAddressInfo">Address & Contacts</a></li>
                                <li><a href="#InsuranceTab" class="PatientAddressInfo">Insurance Info</a></li>
                                <li><a href="#AttachmentTab" class="PatientAddressInfo">Attachment</a></li>
                            </ul>
                            <div id="AddressesTab" class="partialContents">
                                <div id="divAddessAddEditDisabled">

                                    <div class="clear"></div>
                                    <div class="col-xs-12 col-sm-4 col-md-4 col-lg-4">
                                        <div class="rowTable">
                                            <label><span class="mandatoryStar"></span>Relationship Type:</label>
                                            <select id="ddlPersonRelation" name="ddlPersonRelation" class="validate[required]" onchange="GetUserSelfInfo(this, $('#PatientId').val()) "></select>
                                        </div>
                                        <div class="rowTable">
                                            <label><span class="mandatoryStar"></span>First Name:</label>
                                            <input type="text" maxlength="25" id="AddressRelationFirstName" />
                                        </div>
                                        <div class="rowTable">
                                            <label><span class="mandatoryStar"></span>Last Name:</label>
                                            <input type="text" maxlength="25" id="AddressRelationLastName" />
                                        </div>
                                        <div class="form-group">
                                            <label class="control-label" style="width: 200px;">
                                                Is Primary:
                                                <input type="checkbox" id="chkIsPrimary" />
                                            </label>
                                        </div>
                                    </div>
                                    <div class="col-xs-12 col-sm-4 col-md-4 col-lg-4">
                                        <div class="rowTable">
                                            <label><span class="mandatoryStar"></span>Street Address 1:</label>
                                            <input type="text" maxlength="50" id="AddressRelationStreetAddress1" />
                                            @*@Html.TextBoxFor(m => m.CurrentPatientAddressRelation.StreetAddress1, new { @maxlength = "100", @class = "validate[required]" })*@
                                        </div>
                                        <div class="rowTable">
                                            <label>Street address 2:</label>
                                            <input type="text" maxlength="50" id="AddressRelationStreetAddress2" />
                                            @*@Html.TextBoxFor(m => m.CurrentPatientAddressRelation.StreetAddress2, new { @maxlength = "100" })*@
                                        </div>
                                        <div class="rowTable">
                                            <label><span class="mandatoryStar"></span>PO Box:</label>
                                            <input type="text" maxlength="7" id="AddressRelationPOBox" />
                                            @*@Html.TextBoxFor(m => m.CurrentPatientAddressRelation.POBox, new { @maxlength = "10", @class = "validate[required]" })*@
                                        </div>
                                    </div>
                                    <div class="col-xs-12 col-sm-4 col-md-4 col-lg-4">
                                        @*@{ Html.RenderPartial("~/Views/CommonUserControls/CascadingCountryView.cshtml", new BillingSystem.Models.CommonDataView
                                            {
                                                CountryList = new List<BillingSystem.Model.Country>(),
                                                StatesList = new List<BillingSystem.Model.State>(),
                                                CityList = new List<BillingSystem.Model.City>(),
                                                CountryId = Convert.ToInt32(Model.CurrentPatientAddressRelation.CountryID),
                                                StateId = Convert.ToInt32(Model.CurrentPatientAddressRelation.StateID),
                                                CityId = Convert.ToInt32(Model.CurrentPatientAddressRelation.CityID),
                                            }); }*@
                                        <div class="rowTable">
                                            <label class="control-label"><span class="mandatoryStar"></span>Country :</label>
                                            @Html.DropDownListFor(m => m.CurrentPatientAddressRelation.CountryID, new SelectList(Model.CountryList, "CountryID", "CountryName"), "--Select One--", new { id = "ddlCountries", @class = "validate[required]" })
                                        </div>
                                        <div class="rowTable">
                                            <label class="control-label">State :</label>
                                            @Html.DropDownListFor(m => m.CurrentPatientAddressRelation.StateID, new SelectList(Model.StatesList, "Value", "Text"), "--Select One--", new { id = "ddlStates", @class = "validate[required]" })
                                        </div>
                                        <div class="rowTable">
                                            <label class="control-label">City :</label>
                                            @Html.DropDownListFor(m => m.CurrentPatientAddressRelation.CityID, new SelectList(Model.CityList, "Value", "Text"), "--Select One--", new { id = "ddlCities", @class = "validate[required]" })
                                        </div>
                                        <div class="rowTable">
                                            <label><span class="mandatoryStar"></span>Zip Code:</label>
                                            <input type="text" maxlength="7" id="AddressRelationZipCode" />
                                            @*@Html.TextBoxFor(m => m.CurrentPatientAddressRelation.ZipCode, new { @maxlength = "10", @class = "validate[required]" })*@
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div id="InsuranceTab">
                                <div id="divPatientInsurance">
                                    @*<div class="col-sm-12">

                                            <div class="col-sm-4">
                                                <div class="rowTable">
                                                    <label>Company Name :</label>
                                                    <input type="text" id="IsuranceCompanyName" />

                                                </div>Insurance_Startdate
                                            </div>
                                            <div class="col-sm-4">
                                                <div class="rowTable">
                                                    <label>Start Date :</label>
                                                    <input type="text" id="IsuranceCompanyStartdate" />

                                                </div>
                                                <div class="rowTable">
                                                    <label>End Date :</label>
                                                    <input type="text" id="IsuranceCompanyExpirydate" />

                                                </div>
                                            </div>
                                        </div>*@
                                    <div class="col-sm-12" id="divSecondInsuranceForm">

                                        <div class="col-sm-4">
                                            <div class="rowTable">
                                                <label><span class="mandatoryStar"></span>2nd Healthcare Company:</label>
                                                <select id="ddlInsuranceCompany2" class="validate[required]"></select>

                                            </div>
                                            <div class="rowTable">
                                                <label>2nd Healthcare Member ID:</label>
                                                <input type="text" maxlength="10" id="IsuranceCompanyPersonHealthCareNumber2" />

                                            </div>
                                        </div>
                                        <div class="col-sm-4">
                                            <div class="rowTable">
                                                <label><span class="mandatoryStar"></span>2nd Healthcare Insurance Plan:</label>
                                                <select id="ddlInsurancePlan2" class="validate[required]">
                                                    <option>--Select--</option>
                                                </select>

                                            </div>
                                            <div class="rowTable">
                                                <label><span class="mandatoryStar"></span>2nd Healthcare Insurance Policy:</label>
                                                <select id="ddlInsurancePolicy2" class="validate[required]">
                                                    <option>--Select--</option>
                                                </select>

                                            </div>
                                        </div>
                                        <div class="col-sm-4">
                                            <div class="rowTable">
                                                <label><span class="mandatoryStar"></span>2nd Healthcare Start Date:</label>
                                                <input type="text" id="IsuranceCompanyPersonHealthStartDate2" class="dtGeneral validate[required]" />
                                            </div>
                                            <div class="rowTable">
                                                <label><span class="mandatoryStar"></span>2nd Healthcare End Date:</label>
                                                <input type="text" id="IsuranceCompanyPersonHealthEndDate2" class="dtGeneral validate[required]" />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="partialContents" id="AttachmentTab">
                                <div class="col-sm-4">
                                    <div class="rowTable">
                                        <label class="control-label"><span class="mandatoryStar"></span>Documents</label>
                                        <select id="ddlDocumentType" name="ddlDocumentType" class="validate[required]"></select>
                                    </div>
                                </div>
                                <div class="col-sm-4" id="divUploadDocuments">
                                    @{
                                        var filePath = Model != null && !string.IsNullOrEmpty(Model.DocumentsAttachment.FilePath) ? Model.DocumentsAttachment.FilePath : Url.Content("~/images/Noimage.jpg");
                                    }

                                    <div id="fdocupload">
                                        <div class="control-label">
