@using BillingSystem.Common
@using BillingSystem.Models

@model DashboardDisplayOrderView

@{
    ViewBag.Title = "DashboardDisplayOrder View";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="white-bg">
    <div class="panel panel-default collapsecss ourcollapse">
        <div class="panel-heading" data-toggle="collapse" data-parent="#accordion" href="#collapseDashboardDisplayOrderAddEdit">
            <h4 class="panel-title collapseTitle">
                <span class="glyphicon glyphicon-plus-sign"></span> Add DashboardDisplayOrder
                <a class="btn btn-default btn-sm pull-right collapse-btn">
                    <span class="glyphicon glyphicon-chevron-down"></span>
                </a>
            </h4>
        </div>
        <div id="collapseDashboardDisplayOrderAddEdit" class="panel-collapse collapse in">
            <div class="panel-body">
                <div class="base-color">
                    <div class="form">                        
                        @{Html.RenderPartial("~/Views/DashboardDisplayOrder/UserControls/_DashboardDisplayOrderAddEdit.cshtml", Model.CurrentDashboardDisplayOrder);}
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="panel panel-default collapsecss ourcollapse bordertable">
        <div class="panel-heading" data-toggle="collapse" data-parent="#accordion" href="#collapseDashboardDisplayOrderList">
            <h4 class="panel-title collapseTitle">
                <span><img src="../images/list.png" /></span> DashboardDisplayOrder List
                <a class="btn btn-default btn-sm pull-right collapse-btn">
                    <span class="glyphicon glyphicon-chevron-down"></span>
                </a>
            </h4>
        </div>
        <div id="collapseDashboardDisplayOrderList" class="panel-collapse collapse in tableImg">
            <div class="panel-body">
                <div class="base-color" id="DashboardDisplayOrderListDiv">
                    @{Html.RenderPartial("~/Views/DashboardDisplayOrder/UserControls/_DashboardDisplayOrderList.cshtml", Model.DashboardDisplayOrderList);}
                </div>
            </div>
        </div>
    </div>
    <div class="clear"></div>
</div>

<script src="~/Scripts/DashboardDisplayOrder/DashboardDisplayOrder.js?@Helpers.CurrentAssemblyTicks"></script>
<script src="~/Scripts/Common/confirmEvents.js?@Helpers.CurrentAssemblyTicks"></script>