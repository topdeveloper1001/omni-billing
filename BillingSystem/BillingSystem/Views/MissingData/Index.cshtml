@model BillingSystem.Models.MissingDataView

@{
    ViewBag.Title = "MissingXMLData";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="~/Scripts/MissingData/MissingData.js"></script>
<div class="white-bg">
    <div class="panel panel-default collapsecss ourcollapse bordertable">
        <div class="panel-heading" data-toggle="collapse" data-parent="#accordion" href="#collapseTwo">
            <h4 class="panel-title collapseTitle">
                <span><img src="~/images/list.png" /></span>&nbsp;Missing Data List
                <a class="btn btn-default btn-sm pull-right collapse-btn">
                    <span class="glyphicon glyphicon-chevron-down"></span>
                </a>
            </h4>
        </div>
        <div id="collapseTwo" class="panel-collapse collapse in tableImg">
            <div class="panel-body">
                <div class="base-color" id="MedicalVitalListDiv">
                    @{Html.RenderPartial("~/Views/MissingData/UserControls/_MissingDataList.cshtml", Model.MissingDataList);}
                </div>
            </div>
        </div>
    </div>
    <div class="panel panel-default collapsecss ourcollapse bordertable">
        <div class="panel-heading" data-toggle="collapse" data-parent="#accordion" href="#collapseTwo">
            <h4 class="panel-title collapseTitle">
                <span><img src="~/images/list.png" /></span>&nbsp;Bills Ready to scrub
                <a class="btn btn-default btn-sm pull-right collapse-btn">
                    <span class="glyphicon glyphicon-chevron-down"></span>
                </a>
            </h4>
        </div>
        <div id="CollapseBillHeaderList" class="panel-collapse collapse in">
            <div class="table-responsive">
                <div id="BillHeaderListDiv" class="scroller">
                    @{ Html.RenderPartial("~/Views/MissingData/UserControls/_XMLBillHeaderList.cshtml", Model.BillReadyForScrub); }
                </div>
            </div>
        </div>
    </div>
    <div class="panel panel-default collapsecss ourcollapse">
            <div class="panel-heading" data-toggle="collapse" data-parent="#accordion" href="#CollapseBillActivitiesList">
                <h4 class="panel-title collapseTitle" id="BillActivityListHeading">Bill Activities <span id="billnumberSpn"></span>
                    <a class="btn btn-default btn-sm pull-right collapse-btn">
                        <span class="glyphicon glyphicon-chevron-down"></span>
                    </a>
                </h4>
            </div>
            <div id="CollapseBillActivitiesList" class="panel-collapse collapse">
                <div class="table-responsive scroller">
                    <div id="BillActivityListDiv">
                        @*@{ Html.RenderPartial("~/Views/BillHeader/UserControls/_BillActivityList.cshtml", Model.BillActivityList); }*@
                    </div>
                </div>
            </div>
        </div>
    <div class="clear"></div>
</div>