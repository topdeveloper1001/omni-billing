@using BillingSystem.Model.CustomModel
@model List<PatientPreSchedulingCustomModel>

<div class="form">
    <div class="table-responsive">
        @if (Model != null && Model.Count > 0)
        {
            WebGrid grid = new WebGrid(Model, ajaxUpdateContainerId: "gridContent");
            <div id="gridContent">
                @grid.GetHtml(tableStyle: "table table-grid", headerStyle: "gridHead", footerStyle: "gridFooter", rowStyle: "gridRow", alternatingRowStyle: "gridAltRow", columns: grid.Columns(
                                grid.Column("PatientPreSchedulingId", header: "PatientPreSchedulingId" , canSort: true),
                                grid.Column("PatientId", header: "PatientId" , canSort: true),
                                grid.Column("EncounterId", header: "EncounterId" , canSort: true),
                                grid.Column("FacilityId", header: "FacilityId" , canSort: true),
                                grid.Column("PhysicianId", header: "PhysicianId" , canSort: true),
                                grid.Column("PhysicianSpeciality", header: "PhysicianSpeciality" , canSort: true),
                                grid.Column("TypeOfProcedure", header: "TypeOfProcedure" , canSort: true),
                                grid.Column("CorporateId", header: "CorporateId" , canSort: true),
                                grid.Column("Description", header: "Description" , canSort: true),
                                grid.Column("CreatedBy", header: "CreatedBy" , canSort: true),
                                grid.Column("CreatedDate", header: "CreatedDate" , canSort: true),
                                grid.Column("ModifiedBy", header: "ModifiedBy" , canSort: true),
                                grid.Column("ModifiedDate", header: "ModifiedDate" , canSort: true),
                                grid.Column("IsActive", header: "IsActive" , canSort: true),
                                grid.Column("DeletedBy", header: "DeletedBy" , canSort: true),
                                grid.Column("DeletedDate", header: "DeletedDate" , canSort: true),
                                grid.Column("ExtValue1", header: "ExtValue1" , canSort: true),
                                grid.Column("ExtValue2", header: "ExtValue2" , canSort: true),
                                grid.Column("ExtValue3", header: "ExtValue3" , canSort: true),
                                grid.Column("ExtValue4", header: "ExtValue4" , canSort: true),
                                grid.Column("ExtValue5", header: "ExtValue5" , canSort: true),
                                grid.Column("PreferredDate", header: "PreferredDate" , canSort: true),
                                grid.Column("PreferredTimeSlots", header: "PreferredTimeSlots" , canSort: true),
                
                grid.Column(header: "Action", format: @<text>
                <a href="#" onclick="EditPatientPreScheduling('@item.PatientPreSchedulingId')" style="float: left; margin-right: 7px; width: 15px;">
                    <img src="../images/edit.png" /></a>
                                                           &nbsp;
             @*<a href="#" title="Delete" onclick="return DeletePatientPreScheduling('@item.PatientPreSchedulingId');" style="float: left; width: 15px;"><img src="../images/delete.png" /></a>*@
                <a href="javascript:void(0);" title="Delete" onclick="return OpenConfirmPopup('@item.PatientPreSchedulingId','Delete Patient PreScheduling','',DeletePatientPreScheduling,null); " style="float: left; width: 15px;">
                    <img src="@Url.Content("~/images/delete.png")" />
                </a>
                </text>)
                                ));
            </div>            
        }
        else
        {

            <h2>No Record found</h2>
        }
    </div>
</div>
