@model BillingSystem.Models.PhonesView
@using BillingSystem.Common
@using BillingSystem.Model
<style type="text/css">
    .rowTable input, .rowTable select {
        margin-bottom: 10px !important;
    }
</style>
<div class="form">
    <div id="divPhoneAddEdit" class="editform">
        @{ Html.RenderPartial("~/Views/PatientInfo/UserControls/_phoneAddEdit.cshtml", new PatientPhone
           {
               IsDeleted = false,
               IsdontContact = true
           });
        }
    </div>

    <div class="ourcollapse bordertable table-responsive" id="divPhoneGrid">
        @{Html.RenderPartial("~/Views/PatientInfo/UserControls/_phoneGrid.cshtml", Model.Phonelst);}
    </div>
</div>
<script src="~/Scripts/PatientInfo/PatientPhone.js?@Helpers.CurrentAssemblyTicks" type="text/javascript"></script>
<script src="~/Scripts/Common/confirmEvents.js?@Helpers.CurrentAssemblyTicks"></script>
