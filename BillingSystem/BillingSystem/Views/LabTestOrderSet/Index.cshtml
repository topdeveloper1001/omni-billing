@using BillingSystem.Common
@using BillingSystem.Models

@model LabTestOrderSetViews

@{
    ViewBag.Title = "Lab Test OrderSet View";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style type="text/css">
    .home {
        -webkit-box-shadow: 10px 10px 1px 0px;
        margin: 4px 31px 62px 41px;
        border: 6px solid #E6E6FA;
        box-shadow: 10px 10px 1px 0px;
        width: 150px;
        height: 65px;
        float: left;
        text-align: center;
        background-color: #5c7499;
    }
</style>
<div class="white-bg">
    <div class="panel panel-default collapsecss ourcollapse">
        <div class="panel-heading" data-toggle="collapse" data-parent="#accordion" href="#collapseLabTestOrderSetAddEdit">
            <h4 class="panel-title collapseTitle">
                <span class="glyphicon glyphicon-plus-sign"></span>Add Lab Test Order Set
                <a class="btn btn-default btn-sm pull-right collapse-btn">
                    <span class="glyphicon glyphicon-chevron-down"></span>
                </a>
            </h4>
        </div>
        <div id="collapseLabTestOrderSetAddEdit" class="panel-collapse collapse">
            <div class="panel-body">
                <div class="base-color">
                    <div class="form">
                        @{Html.RenderPartial("~/Views/LabTestOrderSet/UserControls/_LabTestOrderSetAddEdit.cshtml", Model.CurrentLabTestOrderSet);}
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="panel panel-default collapsecss ourcollapse bordertable">
        <div class="panel-heading" data-toggle="collapse" data-parent="#accordion" href="#collapseLabTestOrderSetList">
            <h4 class="panel-title collapseTitle">
                <span>
                    <img src="@Url.Content("~/images/list.png")" /></span>Lab Test Order Set List
                <a class="btn btn-default btn-sm pull-right collapse-btn">
                    <span class="glyphicon glyphicon-chevron-down"></span>
                </a>
            </h4>
        </div>
        <div id="collapseLabTestOrderSetList" class="panel-collapse collapse in tableImg">
            <div class="panel-body">
                <div class="base-color" id="LabTestOrderSetListDiv">
                    @{Html.RenderPartial("~/Views/LabTestOrderSet/UserControls/_LabTestOrderSetList.cshtml", Model.LabTestOrderSetList);}
                </div>
            </div>
        </div>
    </div>
    <div class="clear"></div>
</div>

<script src="~/Scripts/LabTestOrderSet/LabTestOrderSetScript.js?@Helpers.CurrentAssemblyTicks"></script>
<script src="~/Scripts/Common/confirmEvents.js?@Helpers.CurrentAssemblyTicks"></script>
