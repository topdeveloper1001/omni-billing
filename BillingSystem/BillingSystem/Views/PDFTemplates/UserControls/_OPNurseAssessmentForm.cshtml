                                    @Html.TextBoxFor(m => m.MeasurementsList[2].Value, new { @ID = "txtBp", @Class = "form-control", maxlength = "10" })
                                }
                                else
                                {
                                    <input type="text" id="txtBp" class="form-control" maxlength="10" />
                                }
                            </td>
                            <td>

                                @if (Model.MeasurementsList.Count > 0)
                                {
                                    @Html.TextBoxFor(m => m.MeasurementsList[3].Value, new { @ID = "txtResp", @Class = "form-control", @maxlength = "10" })

                                }
                                else
                                {
                                    <input type="text" id="txtResp" class="form-control" maxlength="10" />
                                }
                            </td>
                            <td>

                                @if (Model.MeasurementsList.Count > 0)
                                {
                                    @Html.TextBoxFor(m => m.MeasurementsList[4].Value, new { @ID = "txtSpo", @Class = "form-control", @maxlength = "10" })

                                }
                                else
                                {
                                    <input type="text" id="txtSpo" class="form-control" maxlength="10" />
                                }
                            </td>
                            <td>

                                @if (Model.MeasurementsList.Count > 0)
                                {
                                    @Html.TextBoxFor(m => m.MeasurementsList[5].Value, new { @ID = "txtWeightData", @Class = "form-control", @maxlength = "10" })

                                }
                                else
                                {
                                    <input type="text" id="txtWeightData" class="form-control" maxlength="10" />
                                }


                            </td>
                            <td>

                                @if (Model.MeasurementsList.Count > 0)
                                {
                                    @Html.TextBoxFor(m => m.MeasurementsList[6].Value, new { @ID = "txtHeight", @Class = "form-control", @maxlength = "10" })

                                }
                                else
                                {
                                    <input type="text" id="txtHeight" class="form-control" maxlength="10" />
                                }
                            </td>
                            <td>

                                @if (Model.MeasurementsList.Count > 0)
                                {
                                    @Html.TextBoxFor(m => m.MeasurementsList[7].Value, new { @ID = "txtBmi", @Class = "form-control", @maxlength = "10" })

                                }
                                else
                                {
                                    <input type="text" id="txtBmi" class="form-control" maxlength="10" />
                                }


                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
<!-----table-section------->
<div class="row">
    <!-----Allergy-section------->
    <div class="col-sm-12">
        <div class="form_panel">
            <div class="col-sm-12">
                <div class="row">
                    <div class="col-sm-1">
                        <label><strong>ALLERGY:</strong></label>
                    </div>
                    <div class="col-sm-1">
                        @*<input type="checkbox" value="" id="chkNka">*@
                        @(Model.AllergyList.Count > 0 ? Html.CheckBox("chkNka", Model.AllergyList[0].Value == "1", new { @Class = "myCheckboxAllergy" }) : Html.CheckBox("chkNka", false, new { @Class = "myCheckboxAllergy" }))


                        <label>NKA</label>
                    </div>
                    <div class="col-sm-5">
                        <div class="col-sm-2">
                            @*<input type="checkbox" value="" id="chkAllergyYes">*@
                            @(Model.AllergyList.Count > 0 ? Html.CheckBox("chkAllergyYes", Model.AllergyList[1].Value == "1", new { @Class = "myCheckboxAllergy" }) : Html.CheckBox("chkAllergyYes", false, new { @Class = "myCheckboxAllergy" }))


                            <label>Yes</label>
                        </div>
                        <div class="col-sm-10">
                            @if (Model.AllergyList.Count > 0)
                            {
                                @Html.TextBoxFor(m => m.AllergyList[2].Value, new { @ID = "txtAllergySpecification", @Class = "form-control", @maxlength = "50" })
                                
                            }
                            else
                            {
                                <div class="inline_field">
                               <label>Please Specify:</label>                                
                               <input type="text" placeholder="" class="form-control" id="txtAllergySpecification" maxlength="50">
                                    </div>
                            }

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<!-----Allergy-section------->
<div class="row">
    <!----Pain-Assessment-section------->
    <div class="col-sm-12">
        <div class="form_panel">

            <div class="col-sm-12">
                <h1 class="head">Pain Assessment:</h1>
                <div class="row">
                    <div class="col-sm-3"  style="display:none;">
                        <div class="row pain_slider">
                            <div class="col-sm-8">
                                <div class="form-group">
                                    <label class="strong">Enter pain assessment</label>

                                    <div id="slider-range-min" style="width: 100%;"></div>
                                </div>
                            </div>
                            <div class="col-sm-3">
                                <div class="form-group">
                                    @if (Model.PainAssessmentList.Count > 0)
                                    {
                                        @Html.TextBoxFor(m => m.PainAssessmentList[0].Value, new { @ID = "amount", @Class = "form-control", @maxlength = "2" })

                                    }
                                    else
                                    {
                                        <input type="text" id="amount" class="form-control" maxlength="2">
                                    }
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <div class="pain_assessment">
                            <img src="@Url.Content("~/images/faces.jpg")" class="img-responsive">
                            <div class="face face1" face="yes" id="face1"></div>
                            <div class="face face2" face="yes" id="face2"></div>
                            <div class="face face3" face="yes" id="face3"></div>
                            <div class="face face4" face="yes" id="face4"></div>
                            <div class="face face5" face="yes" id="face5"></div>
                            <div class="face face6" face="yes" id="face6"></div>
                
                            @(Model.PainAssessmentLevels.Count > 0 ? Html.CheckBox("chkLevel1", Model.PainAssessmentLevels[0].Value == "1", new { @class = "myCheckPainAssessment level0"}) : Html.CheckBox("chkLevel1", false, new { @class = "myCheckPainAssessment level0" , onclick="return chqchecked(this,'face1');"}))
                            @(Model.PainAssessmentLevels.Count > 0 ? Html.CheckBox("chkLevel2", Model.PainAssessmentLevels[1].Value == "1", new { @class = "myCheckPainAssessment level1"}) : Html.CheckBox("chkLevel2", false, new { @class = "myCheckPainAssessment level1", onclick="return chqchecked(this,'face1');"}))


                            @(Model.PainAssessmentLevels.Count > 0 ? Html.CheckBox("chkLevel3", Model.PainAssessmentLevels[2].Value == "1", new { @class = "myCheckPainAssessment level2" }) : Html.CheckBox("chkLevel3", false, new { @class = "myCheckPainAssessment level2", onclick="return chqchecked(this,'face2');"}))
                            @(Model.PainAssessmentLevels.Count > 0 ? Html.CheckBox("chkLevel4", Model.PainAssessmentLevels[3].Value == "1", new { @class = "myCheckPainAssessment level3" }) : Html.CheckBox("chkLevel4", false, new { @class = "myCheckPainAssessment level3", onclick="return chqchecked(this,'face2');"}))
                            @(Model.PainAssessmentLevels.Count > 0 ? Html.CheckBox("chkLevel5", Model.PainAssessmentLevels[4].Value == "1", new { @class = "myCheckPainAssessment level4" }) : Html.CheckBox("chkLevel5", false, new { @class = "myCheckPainAssessment level4", onclick="return chqchecked(this,'face3');"}))
                            @(Model.PainAssessmentLevels.Count > 0 ? Html.CheckBox("chkLevel6", Model.PainAssessmentLevels[5].Value == "1", new { @class = "myCheckPainAssessment level5" }) : Html.CheckBox("chkLevel6", false, new { @class = "myCheckPainAssessment level5", onclick="return chqchecked(this,'face3');"}))
                            @(Model.PainAssessmentLevels.Count > 0 ? Html.CheckBox("chkLevel7", Model.PainAssessmentLevels[6].Value == "1", new { @class = "myCheckPainAssessment level6" }) : Html.CheckBox("chkLevel7", false, new { @class = "myCheckPainAssessment level6", onclick="return chqchecked(this,'face4');"}))
                            @(Model.PainAssessmentLevels.Count > 0 ? Html.CheckBox("chkLevel8", Model.PainAssessmentLevels[7].Value == "1", new { @class = "myCheckPainAssessment level7" }) : Html.CheckBox("chkLevel8", false, new { @class = "myCheckPainAssessment level7", onclick="return chqchecked(this,'face4');"}))
                            @(Model.PainAssessmentLevels.Count > 0 ? Html.CheckBox("chkLevel9", Model.PainAssessmentLevels[8].Value == "1", new { @class = "myCheckPainAssessment level8" }) : Html.CheckBox("chkLevel9", false, new { @class = "myCheckPainAssessment level8", onclick="return chqchecked(this,'face5');"}))
                            @(Model.PainAssessmentLevels.Count > 0 ? Html.CheckBox("chkLevel10", Model.PainAssessmentLevels[9].Value == "1", new { @class = "myCheckPainAssessment level9" }) : Html.CheckBox("chkLevel10", false, new { @class = "myCheckPainAssessment level9", onclick="return chqchecked(this,'face5');"} ))
                            @(Model.PainAssessmentLevels.Count > 0 ? Html.CheckBox("chkLevel11", Model.PainAssessmentLevels[10].Value == "1", new { @class = "myCheckPainAssessment level10" }) : Html.CheckBox("chkLevel11", false, new { @class = "myCheckPainAssessment level10", onclick="return chqchecked(this,'face6');"} ))
                        </div>
                    </div>



                    <div class="col-sm-12">
                        <p>(If Pain identified (1 or more) complete below information. If pain score is more than 4-fill NSF35)</p>
                    </div>
                    <div class="inner_block">
                    <div class="col-sm-6">
                        <div class="form-group">
                            <div class="row">
                                <div class="col-sm-5">
                                    <label><strong>Location:</strong></label>
                                </div>
                                <div class="col-sm-7">
                                    @if (Model.PainAssessmentList.Count > 0)
                                    {
                                        @Html.TextBoxFor(m => m.PainAssessmentList[1].Value, new { @ID = "textLocation", @Class = "form-control", @maxlength = "50" })
                                    
                                    }
                                    else
                                    {
                                        <input type="text" class="form-control" id="textLocation" maxlength="50" name="" value="">
                                    }
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class=" col-sm-6">
                        <div class="form-group">
                            <div class="row">
                                <div class="col-sm-5">
                                    <label><strong>Check Character Codes Below:</strong></label>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                    <div class=" col-sm-6">
                        <div class="form-group">
                            <div class="row">
                                <div class="col-sm-8">
                                    <label><strong>Frequency</strong>(how often do you experience pain?)</label>
                                </div>
                                <div class="col-sm-4">
                                    @if (Model.PainAssessmentList.Count > 0)
                                    {
                                        @Html.TextBoxFor(m => m.PainAssessmentList[2].Value, new { @ID = "textFrequency", @Class = "form-control", @maxlength = "30" })
                                    
                                    }
                                    else
                                    {
                                        <input type="text" class="form-control" id="textFrequency" maxlength="30" name="FromAddress1" value="">
                                    
                                    }
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <div class="form-group">
                            <div class="row">
                                <div class="col-sm-2">
                                    <label><strong>Pattern:</strong></label>
                                </div>
                                <div class="col-sm-3">
                                    @*<input type="checkbox" value="" id="chkConstant">*@
                                    @(Model.PainAssessmentList.Count > 0 ? Html.CheckBox("chkConstant", Model.PainAssessmentList[4].Value == "1", new { @Class = "myCheckboxPattern" }) : Html.CheckBox("chkConstant", false, new { @Class = "myCheckboxPattern" }))


                                    <label>Constant</label>
                                </div>
                                <div class="col-sm-3">
                                    @*<input type="checkbox" value="" id="chkIntermittent">*@
                                    @(Model.PainAssessmentList.Count > 0 ? Html.CheckBox("chkIntermittent", Model.PainAssessmentList[5].Value == "1", new { @Class = "myCheckboxPattern" }) : Html.CheckBox("chkIntermittent", false, new { @Class = "myCheckboxPattern" }))
                                    <label>Intermittent</label>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                    <div class=" col-sm-8">
                        <div class="form-group">
                            <div class="row">
                                <div class="col-sm-6">
                                    <label><strong>Duration</strong>(how long do you had this pain?)</label>
                                </div>
                                <div class="col-sm-3">
                                    @if (Model.PainAssessmentList.Count > 0)
                                    {
                                        @Html.TextBoxFor(m => m.PainAssessmentList[3].Value, new { @ID = "textPainDuration", @Class = "form-control", @maxlength = "30" })
                                    
                                    }
                                    else
                                    {
                                        <input type="text" value="" name="FromAddress1" maxlength="30" id="textPainDuration" class="form-control">
                                    
                                    }
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-12">
                        <div class="form-group">
                            <div class="row">
                                <div class="col-sm-4 row">
                                    <label class="col-sm-6"><strong>Radiation:</strong></label>
                                    @*<input type="checkbox" value="" id="chkRadiationNo">*@
                                    @(Model.PainAssessmentList.Count > 0 ? Html.CheckBox("chkRadiationNo", Model.PainAssessmentList[6].Value == "1", new { @Class = "myCheckboxRadiation" }) : Html.CheckBox("chkRadiationNo", false, new { @Class = "myCheckboxRadiation" }))


                                    <label>NO</label>
                                    &nbsp;
                                     @*<input type="checkbox" value="" id="chkRadiationYes">*@
                                    @(Model.PainAssessmentList.Count > 0 ? Html.CheckBox("chkRadiationYes", Model.PainAssessmentList[7].Value == "1", new { @Class = "myCheckboxRadiation" }) : Html.CheckBox("chkRadiationYes", false, new { @Class = "myCheckboxRadiation" }))


                                    <label>Yes: Specify Where</label>
                                </div>

                                <div class="col-sm-2">

                                    <div class="">
