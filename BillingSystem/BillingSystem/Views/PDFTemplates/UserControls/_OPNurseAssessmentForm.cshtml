                                    @(Model.RiskList.Count > 0 ? Html.CheckBox("chkBedInLowest", Model.RiskList[16].Value == "1") : Html.CheckBox("chkBedInLowest", false))
                                    <label>Bed in lowest position with wheels locked and side rail up</label>
                                    </li>


                                    <li>
                                    @*<input type="checkbox" value="" class="checkbox-mt" id="chkIdentify">*@
                                    @(Model.RiskList.Count > 0 ? Html.CheckBox("chkIdentify", Model.RiskList[17].Value == "1") : Html.CheckBox("chkIdentify", false))
                                    <label>Identify patient with star sticker on the file, door and white board </label>
                                    </li>
                                </ul>

                            </td>
                        </tr>
                    </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
