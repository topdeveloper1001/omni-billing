@using Gma.QrCodeNet.Encoding.DataEncodation
@model BillingSystem.Models.PDFTemplatesView
<link href="~/css/pdf_style.css" rel="stylesheet" />
<style>
    input[type="checkbox"] {
        margin: -3px 0 0;
    }

    .table-bordered > thead > tr > th, .table-bordered > tbody > tr > th, .table-bordered > tfoot > tr > th, .table-bordered > thead > tr > td, .table-bordered > tbody > tr > td, .table-bordered > tfoot > tr > td {
        border: 1px solid #acacac;
    }
</style>
<div class="row">
    <!-----main-header------->
    <div class="col-sm-12">
        <div class="form_panel">
            <div class="col-sm-2">
                <img src="../images/omni.png" class="img-responsive" alt="">
            </div>
            <div class="col-sm-8">
                <div class="form-outer">
                    <div class="row">
                        <div class="form-group">
                            <div class="col-sm-4"><span>Form No:</span><span>NSF27</span></div>
                            <div class="col-sm-4">
                                <input type="text" class="form-control" placeholder="" id="txtFormNumber" value="@Model.FormNumber" readonly="">
                            </div>
                        </div>
                        <div class="col-sm-12">
                            <h1 class="outpatient-heading">OUTPATIENT NURSING ASSESSMENT FORM - Adult</h1>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-sm-6">
                            <div class="form-group">
                                <label class="col-sm-5">Encounter ID</label>
                                <div class="col-sm-7">
                                    <input type="text" value="@Model.EncounterId" name="FromAddress1" maxlength="30" id="txtEncounterId" class="form-control" readonly>
                                </div>
                            </div>
                        </div>
                        <div class=" col-sm-6">
                            <div class="form-group">
                                <div class="col-sm-5">
                                    <label>Patient Last Name</label>
                                </div>
                                <div class="col-sm-7">
                                    <input type="text" value="@Model.PatientLastName" id="txtPatientLastName" name="FromAddress1" maxlength="30" class="form-control" readonly>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class=" col-sm-6">
                            <div class="form-group">
                                <div class="col-sm-5">
                                    <label>Date of Admission</label>
                                </div>
                                <div class="col-sm-7">
                                    <input type="text" value="@DateTime.Now.ToString()" maxlength="30" id="txtAdmissionDate" class="form-control" readonly>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group">
                                <div class="col-sm-5">
                                    <label>First Name</label>
                                </div>
                                <div class="col-sm-7">
                                    <input type="text" value="@Model.PatientFirstName" name="" maxlength="30" id="FromAddress1" class="form-control" readonly>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">

                        <div class="col-sm-6">
                            <div class="form-group">
                                <div class="col-sm-5">
                                    <label>DOB</label>
                                </div>
                                <div class="col-sm-7">
                                    <input type="text" value="@Model.DateOfBirth" name="FromAddress1" maxlength="30" id="txtDateOfBirth" class="form-control" readonly>
                                </div>
                            </div>
                        </div>
                        <div class=" col-sm-6">
                            <div class="form-group">
                                <div class="col-sm-5">
                                    <label>Age</label>
                                </div>
                                <div class="col-sm-2">
                                    <input type="text" value="@Model.PersonAge" name="FromAddress1" maxlength="30" id="txtPatientAge" class="form-control" readonly>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>
</div>
<!-----main-header-ends------->
<div class="row">
    <!-----appointment-section------->
    <div class="col-sm-12">
        <div class="form_panel">
            <div class="col-sm-4">
                <div class="row">
                    <div class="col-sm-12">
                        <div class="form-group">
                            <label>Date/Time</label>
                            @if (Model.VisitDetailList.Count > 0)
                            {
                                @Html.TextBoxFor(m => m.VisitDetailList[0].Value, new { @ID = "txtCurrentDate", @Class = "form-control dtGenera" })
                            
                            }
                            else
                            {
                                <input type="text" placeholder="" class="form-control dtGeneral" id="txtCurrentDate">
                            }

                        </div>

                        <div class="form-group">
                            <label>Reason For Visit</label>
                            @if (Model.VisitDetailList.Count > 0)
                            {
                                @Html.TextBoxFor(m => m.VisitDetailList[1].Value, new { @ID = "txtReasonOfVisit", @Class = "form-control", @maxlength = "100" })

                            }
                            else
                            {
                                <input type="text" placeholder="" class="form-control" id="txtReasonOfVisit" maxlength="100">

                            }

                        </div>
                    </div>
                </div>
            </div>
            <div class="col-sm-6 col-lg-6">
                <div class="form-group">
                    <div class="col-sm-12">
                        <label><strong>Appointment ?</strong></label>
                    </div>
                    <div class="col-sm-12">
                        <div class="row">

                        <div class="col-sm-2">
                            @*<input type="checkbox" value="" class="checkbox-mt" id="chkAppointmentNo">*@
                            @(Model.VisitDetailList.Count > 0 ? Html.CheckBox("chkAppointmentNo", Model.VisitDetailList[2].Value == "1", new { @Class = "myCheckboxAppointment", @onclick = "UncheckNewPatientCheckBox();" }) : Html.CheckBox("chkAppointmentNo", false, new { @Class = "myCheckboxAppointment", @onclick = "UncheckNewPatientCheckBox();" }))
                            <label>No</label>
                        </div>
                        <div class="col-sm-7">
                            @*<input type="checkbox" value="" class="checkbox-mt" id="chkAppointmentYes">*@
                            @(Model.VisitDetailList.Count > 0 ? Html.CheckBox("chkAppointmentYes", Model.VisitDetailList[3].Value == "1", new { @Class = "myCheckboxAppointment" }) : Html.CheckBox("chkAppointmentYes", false, new { @Class = "myCheckboxAppointment"}))
                            <label>Yes:Full Outpatient Assessement Required?</label>
                        </div>
                        <div class="col-sm-2">
                            @*<input type="checkbox" value="" class="checkbox-mt" id="chkAssessementNo">*@
                            @(Model.VisitDetailList.Count > 0 ? Html.CheckBox("chkAssessementNo", Model.VisitDetailList[4].Value == "1", new { @Class = "appointmentRequired", @onclick = "UncheckNewPatientCheckBox();" }) : Html.CheckBox("chkAssessementNo", false, new { @Class = "appointmentRequired", @onclick = "UncheckNewPatientCheckBox();" }))
                            <label>No</label>
                        </div>
                    </div>
                </div>
            </div>
            </div>
            <div class="col-sm-6">
                <div class="form-group grayBackground">
                    <div class="col-sm-12">
                        @(Model.VisitDetailList.Count > 0 ? Html.CheckBox("chkAssessementYes", Model.VisitDetailList[5].Value == "1", new { @Class = "appointmentRequired" }) : Html.CheckBox("chkAssessementYes", false, new { @Class = "appointmentRequired" }))
                        <label>IF Yes:</label>
                    </div>
                    <div class="col-sm-3">
                        @*<input type="checkbox" value="" id="chkNewPatient">*@
                        @(Model.VisitDetailList.Count > 0 ? Html.CheckBox("chkNewPatient", Model.VisitDetailList[6].Value == "1", new { @Class = "myCheckPainBoxNewPatient" }) : Html.CheckBox("chkNewPatient", false, new { @Class = "myCheckPainBoxNewPatient" }))
                        <label>New patient</label>
                    </div>
                    <div class="col-sm-4">
                        @*<input type="checkbox" value="" id="chkCondition">*@
                        @(Model.VisitDetailList.Count > 0 ? Html.CheckBox("chkCondition", Model.VisitDetailList[7].Value == "1", new { @Class = "myCheckPainBoxNewPatient" }) : Html.CheckBox("chkCondition", false, new { @Class = "myCheckPainBoxNewPatient" }))
                        <label>Change in Condition</label>
                    </div>
                    <div class="col-sm-5">
                        @*<input type="checkbox" value="" id="chkLastVisit">*@
                        @(Model.VisitDetailList.Count > 0 ? Html.CheckBox("chkLastVisit", Model.VisitDetailList[8].Value == "1", new { @Class = "myCheckPainBoxNewPatient" }) : Html.CheckBox("chkLastVisit", false, new { @Class = "myCheckPainBoxNewPatient" }))
                        <label>Last visit more than 12 months</label>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-----appointment-section-ends------>
