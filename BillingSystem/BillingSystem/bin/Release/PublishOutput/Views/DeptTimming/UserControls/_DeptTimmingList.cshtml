@using BillingSystem.Model.CustomModel
@model List<DeptTimmingCustomModel>

<div class="form">
    <div class="table-responsive">
        @if (Model != null && Model.Count > 0)
        {
            WebGrid grid = new WebGrid(Model, ajaxUpdateContainerId: "gridContent");
            <div id="gridContent">
                @grid.GetHtml(tableStyle: "table table-grid", headerStyle: "gridHead", footerStyle: "gridFooter", rowStyle: "gridRow", alternatingRowStyle: "gridAltRow", columns: grid.Columns(
                                grid.Column("TimingId", header: "TimingId" , canSort: true),
                                grid.Column("FacilityStructureID", header: "FacilityStructureID" , canSort: true),
                                grid.Column("OpeningDayId", header: "OpeningDayId" , canSort: true),
                                grid.Column("OpeningTime", header: "OpeningTime" , canSort: true),
                                grid.Column("ClosingTime", header: "ClosingTime" , canSort: true),
                                grid.Column("TrunAroundTime", header: "TrunAroundTime" , canSort: true),
                                grid.Column("IsActive", header: "IsActive" , canSort: true),
                                grid.Column("CreatedBy", header: "CreatedBy" , canSort: true),
                                grid.Column("CreatedDate", header: "CreatedDate" , canSort: true),
                                grid.Column("ModifiedBy", header: "ModifiedBy" , canSort: true),
                                grid.Column("ModifiedDate", header: "ModifiedDate" , canSort: true),
                                grid.Column("ExtValue1", header: "ExtValue1" , canSort: true),
                                grid.Column("ExtValue2", header: "ExtValue2" , canSort: true),
                
                grid.Column(header: "Action", format: @<text>
                <a href="#" onclick="EditDeptTimming('@item.DeptTimmingId')" style="float: left; margin-right: 7px; width: 15px;">
                    <img src="../images/edit.png" /></a>
                                                           &nbsp;
                @*<a href="#" title="Delete" onclick="return DeleteDeptTimming('@item.DeptTimmingId');" style="float: left; width: 15px;"><img src="../images/delete.png" /></a>*@
                <a href="javascript:void(0);" title="Delete" onclick="return OpenConfirmPopup('@item.DeptTimmingId','Delete Department Timming','',DeleteDeptTimming,null); " style="float: left; width: 15px;">
                    <img src="@Url.Content("~/images/delete.png")" />
                </a>                                   
                </text>)
                                ));
            </div>            
        }
        else
        {

            <h2>No Record found</h2>
        }
    </div>
</div>
