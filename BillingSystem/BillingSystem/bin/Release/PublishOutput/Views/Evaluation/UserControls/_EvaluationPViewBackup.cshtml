@using BillingSystem.Model
@using BillingSystem.Model.CustomModel
@using BillingSystem.Models
@model EvaluationViews

@{
    ViewBag.Title = "Evaluation";
}
@* Patient Summary Tab View *@



<style>
    input[type="checkbox"] {
        margin: 0;
    }

    .table-grid {
        width: 100% !important;
    }
</style>

<script>
    $(function () {
        BindGlobalCodesWithValue("#ddlConsult", 4549, '');
        var currentdate = new Date();
        var datewithFormat = currentdate.format('mm/dd/yyyy');
        if ($('#txtDateEM').val() == '') {
            $('#txtDateEM').val(datewithFormat);
        }
    });
</script>

<div class="white-bg overflowHidden">
    <div class="title">
        <div class="row">
            <div class="col-sm-5">
                <h1>Evaluation and Management Form</h1>
            </div>
        </div>
    </div>
    <div>
        <div class="row">
            <div class="col-md-12">
                <div class="highlight_block">
                    <div class="evaluation_date">
                        <label>Date</label>
                        <input type="text" id="txtDateEM" class="DateTime" /><span></span>
                    </div>
                    <div class="evaluation_time">
                        <label>Time: @DateTime.Now.ToShortTimeString()</label><span></span>
                    </div>
                </div>
            </div>
        </div>

        <div class="scroller2">
            <div class="panel panel-default collapsecss ourcollapse bordertable">
                <div href="#16Collapse" data-parent="#accordion" data-toggle="collapse" class="panel-heading">
                    <h4 class="panel-title collapseTitle">
                        <span>
                            <img src="/images/list.png">
                        </span>
                        Basic Information
                   
                    <a class="btn btn-default btn-sm pull-right collapse-btn">
                        <span class="glyphicon glyphicon-chevron-down"></span>
                    </a>
                    </h4>
                </div>
                <div class="panel-collapse collapse in" id="16Collapse">
                    <div class="panel-body scroller">
                        <div class="base-color">
                            <div class="form">
                                <div class="col-lg-12">
                                    <div class="checkbox_block">
                                        <h2>Types of Patients</h2>
                                        <div class="row">
                                            <div class="col-sm-3">
                                                @*<input id="chkNewPatient" type="checkbox" />*@
                                                @(Model.BasicInfoItems.Count > 0 ? Html.CheckBox("chkNewPatient", Model.BasicInfoItems[0].Value == "1") : Html.CheckBox("chkNewPatient", false))

                                                New Patient
                                            </div>
                                            <div class="col-sm-3">
                                                @*<input id="chkEstPatient" type="checkbox" />*@
                                                @(Model.BasicInfoItems.Count > 0 ? Html.CheckBox("chkEstPatient", Model.BasicInfoItems[1].Value == "1") : Html.CheckBox("chkEstPatient", false))
                                                Established Patient
                                            </div>
                                            <div class="col-sm-3">
                                                @*<input id="chkPatientNonVerbal" type="checkbox" />*@
                                                @(Model.BasicInfoItems.Count > 0 ? Html.CheckBox("chkPatientNonVerbal", Model.BasicInfoItems[2].Value == "1") : Html.CheckBox("chkPatientNonVerbal", false))
                                                Patient nonverbal
                                            </div>
                                        </div>

                                    </div>
                                    <div class="checkbox_block">
                                        <h2>History obtained from </h2>
                                        <div class="row">
                                            <div class="col-sm-3">

                                                @*<input id="chkFamily" type="checkbox" />*@
                                                @(Model.BasicInfoItems.Count > 0 ? Html.CheckBox("chkFamily", Model.BasicInfoItems[3].Value == "1") : Html.CheckBox("chkFamily", false))
                                                Family
                                            </div>
                                            <div class="col-sm-3">
                                                @*<input id="chkMedPersonnel" type="checkbox" />*@
                                                @(Model.BasicInfoItems.Count > 0 ? Html.CheckBox("chkMedPersonnel", Model.BasicInfoItems[4].Value == "1") : Html.CheckBox("chkMedPersonnel", false))
                                                Medical Personnel
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="panel panel-default collapsecss ourcollapse bordertable">
                <div href="#1Collapse" data-parent="#accordion" data-toggle="collapse" class="panel-heading">
                    <h4 class="panel-title collapseTitle">
                        <span>
                            <img src="/images/list.png">
                        </span>
                        Chief Complaint/Reason for Consult:
                   
                    <a class="btn btn-default btn-sm pull-right collapse-btn">
                        <span class="glyphicon glyphicon-chevron-down"></span>
                    </a>
                    </h4>
                </div>
                <div class="panel-collapse collapse in" id="1Collapse">
