                                                <input type="hidden" id="hfRowExists" value="1" />
                                                <input id="hfConsult" type="hidden"  value='@Model.ChiefComplaintItems[0].Value'/>
                                                @Html.TextAreaFor(m => m.ChiefComplaintItems[1].Value, new { cols = "50", rows = "5", @ID = "txtConsultOthers", @Class="form-control" })
                                            }
                                            else
                                            {
