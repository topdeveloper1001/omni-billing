@using BillingSystem.Model.CustomModel
@model List<MedicalNecessityCustomModel>

<div class="form">
    <div class="table-responsive">
        @if (Model != null && Model.Count > 0)
        {
            WebGrid grid = new WebGrid(Model, ajaxUpdateContainerId: "gridContent");
            <div id="gridContent">
                @grid.GetHtml(tableStyle: "table", headerStyle: "gridHead", footerStyle: "gridFooter", rowStyle: "gridRow", alternatingRowStyle: "gridAltRow", columns: grid.Columns(
                                //grid.Column("BillingCode", header: "BillingCode" , canSort: true),
                                //grid.Column("BillingCodeType", header: "BillingCodeType" , canSort: true),
                                   grid.Column("Description", header: "Description", canSort: true),
                                
                                grid.Column("ICD9Code", header: "ICD9Code" , canSort: true),
                                
                grid.Column(header: "Action", format: @<text>
                <a href="#" onclick="EditMedicalNecessity('@item.Id')" style="float: left; margin-right: 7px; width: 15px;">
                    <img src="../images/edit.png" /></a>
                &nbsp;<a href="#" title="Delete" onclick="return DeleteMedicalNecessity('@item.Id');" style="float: left; width: 15px;"><img src="../images/delete.png" /></a>
                </text>)
                                ))
            </div>            
        }
        else
        {

            <h2>No Record found</h2>
        }
    </div>
</div>
