@using BillingSystem.Model.CustomModel
@model List<PatientInfoChangesQueueCustomModel>

<div class="form">
    <div class="table-responsive">
        @if (Model != null && Model.Count > 0)
        {
            WebGrid grid = new WebGrid(Model, ajaxUpdateContainerId: "gridContent");
            <div id="gridContent">
                @grid.GetHtml(tableStyle: "table table-grid", headerStyle: "gridHead", footerStyle: "gridFooter", rowStyle: "gridRow", alternatingRowStyle: "gridAltRow", columns: grid.Columns(
                                grid.Column("Id", header: "Id" , canSort: true),
                                grid.Column("TableName", header: "TableName" , canSort: true),
                                grid.Column("ColumnName", header: "ColumnName" , canSort: true),
                                grid.Column("OldValue", header: "OldValue" , canSort: true),
                                grid.Column("NewValue", header: "NewValue" , canSort: true),
                                grid.Column("GlobalCodeValue", header: "GlobalCodeValue" , canSort: true),
                                grid.Column("GlobalCodeCategory", header: "GlobalCodeCategory" , canSort: true),
                                grid.Column("Status", header: "Status" , canSort: true),
                                grid.Column("PatientId", header: "PatientId" , canSort: true),
                                grid.Column("CorporatId", header: "CorporatId" , canSort: true),
                                grid.Column("FacilityId", header: "FacilityId" , canSort: true),
                                grid.Column("CreatedBy", header: "CreatedBy" , canSort: true),
                                grid.Column("CreatedDate", header: "CreatedDate" , canSort: true),
                                grid.Column("ModifiedBy", header: "ModifiedBy" , canSort: true),
                                grid.Column("ModifiedDate", header: "ModifiedDate" , canSort: true),
                                grid.Column("ApprovedBy", header: "ApprovedBy" , canSort: true),
                                grid.Column("ApprovedDate", header: "ApprovedDate" , canSort: true),
                                grid.Column("IsActive", header: "IsActive" , canSort: true),
                                grid.Column("ExternalValue1", header: "ExternalValue1" , canSort: true),
                                grid.Column("ExternalValue2", header: "ExternalValue2" , canSort: true),

                grid.Column(header: "Action", format: @<text>
                <a href="#" onclick="EditPatientInfoChangesQueue('@item.PatientInfoChangesQueueId')" style="float: left; margin-right: 7px; width: 15px;">
                    <img src="@Url.Content("~/images/edit.png")" /></a>
                <a href="#" title="Delete" onclick="return DeletePatientInfoChangesQueue('@item.PatientInfoChangesQueueId');" style="float: left; width: 15px;">
                    <img src="@Url.Content("~/images/delete.png")" /></a>
                </text>)
                                ));
            </div>            
        }
        else
        {

            <h2>No Record found</h2>
        }
    </div>
</div>
